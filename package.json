{
  "name": "react-webpack-starter",
  "version": "0.0.1",
  "description": "react starter kit built with react + redux + gulp + webpack",
  "author": "nsarafa <nicksarafa@gmail.com>",
  "license": "MIT",
  "main": "index",
  "scripts": {
    "start": "gulp",
    "test": "gulp test"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "import"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "generator-star-spacing": [
        "error",
        {
          "before": true,
          "after": false
        }
      ],
      "semi": 0,
      "eol-last": 0,
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    }
  },
  "babel": {
    "presets": [
      "latest",
      "react"
    ],
    "compact": false
  },
  "dependencies": {
    "babel-core": "^6.18.2",
    "babel-polyfill": "^6.16.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "immutable": "^3.8.1",
    "react": "^15.3.2",
    "react-css-modules": "^3.7.10",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-immutable": "^3.0.8",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.7",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-mocha": "^3.0.1",
    "gulp-rucksack": "^0.1.3",
    "gulp-sass": "^2.3.2",
    "gulp-sourcemaps": "^2.2.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack-stream": "^3.2.0"
  },
  "repository": {
    "url": "git+ssh://git@github.com/nsarafa/react-webpack-starter.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/nsarafa/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/nsarafa/react-webpack-starter#readme",
  "keywords": [
    "Gulp",
    "React",
    "Redux",
    "Webpack"
  ]
}
